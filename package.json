{
  "name": "bookmarkIt",
  "title": "BookmarkIt",
  "description": "Сервис для хранения и организации ваших закладок",
  "keywords": "закладки, избранное, bookmarks, favorites",
  "version": "1.2.0",
  "author": "Artem Ershov",
  "license": "ISC",
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true,
      "jquery": true
    },
    "extends": "eslint:recommended",
    "globals": {
      "angular": true
    },
    "rules": {
      "indent": [
        "error",
        2,
        {
          "SwitchCase": 1
        }
      ],
      "linebreak-style": [
        "error",
        "unix"
      ],
      "quotes": [
        "error",
        "single"
      ],
      "semi": [
        "error",
        "always"
      ]
    }
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "browser-sync": "^2.23.2",
    "connect-history-api-fallback": "^1.5.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^4.1.0",
    "gulp-babel": "^7.0.0",
    "gulp-cached": "^1.1.1",
    "gulp-chokidar": "0.0.5",
    "gulp-clean": "^0.3.2",
    "gulp-clean-css": "^3.9.2",
    "gulp-concat": "^2.6.1",
    "gulp-debug": "^3.2.0",
    "gulp-filter": "^5.1.0",
    "gulp-if": "^2.0.2",
    "gulp-imagemin": "^4.1.0",
    "gulp-newer": "^1.3.0",
    "gulp-ng-html2js": "^0.2.2",
    "gulp-notify": "^3.1.0",
    "gulp-plumber": "^1.1.0",
    "gulp-pug": "^3.3.0",
    "gulp-remember": "^1.0.1",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^3.1.0",
    "gulp-sourcemaps": "^2.6.3",
    "gulp-uglify": "^3.0.0",
    "imagemin-jpeg-recompress": "^5.1.0",
    "main-yarn-files": "^3.0.4",
    "run-sequence": "^2.2.1"
  },
  "dependencies": {
    "angular": "^1.6.8",
    "angular-drag-and-drop-lists": "^2.1.0",
    "angular-route": "^1.6.8",
    "bootstrap": "^3.3.7",
    "bootswatch": "^3.3.7",
    "font-awesome": "^4.7.0",
    "jquery": "^3.2.1"
  },
  "overrides": {
    "angular": {
      "dependencies": {
        "jquery": "^3.2.1"
      }
    },
    "angular-drag-and-drop-lists": {
      "dependencies": {
        "angular": "^1.6.8"
      }
    },
    "bootstrap": {
      "main": [
        "./dist/js/bootstrap.js",
        "./dist/css/bootstrap.css",
        "./dist/fonts/*"
      ],
      "dependencies": {
        "jquery": "^3.2.1"
      }
    },
    "bootswatch": {
      "main": [
        "./flatly/bootstrap.css"
      ],
      "dependencies": {
        "bootstrap": "^3.3.7"
      }
    },
    "font-awesome": {
      "main": [
        "./css/font-awesome.css",
        "./fonts/*"
      ]
    }
  }
}
